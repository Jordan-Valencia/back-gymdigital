{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "prisma/schema.prisma",
          "prisma/migrations/**",
          "node_modules/.prisma/client/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/main.js",
      "method": ["GET","PUT","PATCH","DELETE","POST"]
    }
  ],
  "env": {
    "DATABASE_URL": "@database_url"
  }
}
